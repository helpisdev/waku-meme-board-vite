{
  "name": "waku-meme-board",
  "version": "0.0.1",
  "private": true,
  "description": "Waku Meme Board built with vite",
  "type": "module",
  "scripts": {
    "analyze": "pnpx vite-bundle-visualizer -o stats.html",
    "build": "tsc && vite build",
    "build-storybook": "storybook build",
    "build:types": "tsc --noEmit --pretty",
    "chromatic": "eval $(shx grep VITE_CHROMATIC_PROJECT_TOKEN .env) && pnpx chromatic --project-token=$VITE_CHROMATIC_PROJECT_TOKEN",
    "clean": "rimraf dist",
    "commitlint": "pnpx --no -- commitlint --edit",
    "dev": "vite",
    "format": "prettier --check \"**/*.{ts,tsx,js,jsx,md,html,css}\"",
    "format:fix": "prettier --write \"**/*.{ts,tsx,js,jsx,md,html,css}\"",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint ./src/**/*.tsx ./src/**/*.ts ./tests/**/*.tsx ./tests/**/*.ts --fix",
    "lint:staged": "pnpx lint-staged",
    "madge": "pnpx madge --extensions ts,tsx,js,jsx,css,html,md,mdx ./ --exclude 'vite.config.js|test-setup.js|tailwind.config.js|release.config.js|README.md|postcss.config.js|playwright.config.js|pnpm-lock.yaml|lint-staged.config.js|jsconfig.json|commitlint.config.js|e2e/|dist|.husky|.github|node_modules|.storybook/|.eslintrc.js' --image graph.svg",
    "prepare": "husky install",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "test": "vitest watch",
    "test-storybook": "test-storybook",
    "test:ci": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e:headless": "playwright test",
    "test:ui": "pnpx vitest --ui",
    "test:ui:e2e": "playwright test --ui"
  },
  "dependencies": {
    "@helia/interface": "^2.0.0",
    "@helia/unixfs": "^1.4.1",
    "@libp2p/bootstrap": "^9.0.5",
    "@libp2p/interface-peer-discovery": "^2.0.0",
    "@multiformats/multiaddr": "^12.1.7",
    "@nanostores/react": "^0.7.1",
    "@radix-ui/colors": "^2.1.0",
    "@radix-ui/react-form": "^0.0.3",
    "@sentry/browser": "^7.66.0",
    "@sentry/react": "^7.66.0",
    "@waku/interfaces": "^0.0.17",
    "@waku/react": "0.0.4-b736e79",
    "@waku/sdk": "^0.0.18",
    "blockstore-idb": "^1.1.3",
    "cva": "npm:class-variance-authority@^0.7.0",
    "datastore-idb": "^2.1.3",
    "datastore-level": "^10.1.3",
    "helia": "^2.0.1",
    "i18next": "^23.4.6",
    "i18next-browser-languagedetector": "^7.1.0",
    "multiformats": "^12.1.0",
    "nanostores": "^0.9.3",
    "protobufjs": "^7.2.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^13.2.1",
    "tailwind-merge": "^1.14.0",
    "wouter": "^2.11.0"
  },
